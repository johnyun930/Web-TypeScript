","name":null,"original":{"line":62,"column":0},"generated":{"line":74,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":62,"column":0},"generated":{"line":74,"column":9}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":62,"column":0},"generated":{"line":74,"column":17}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":75,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":75,"column":1}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":75,"column":3}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":77,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":77,"column":7}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":77,"column":8}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":77,"column":16}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":77,"column":19}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":3,"column":13},"generated":{"line":77,"column":27}}],"sources":{"src/views/UserForm.ts":"import { User } from \"../models/User\";\r\n\r\nexport class UserForm{\r\n    constructor(public parent: Element, public model: User){\r\n        this.model.on('change',()=>{\r\n            this.bindModel();\r\n        })\r\n    };\r\n\r\n    bindModel(): void{\r\n        this.model.on('change',()=>{\r\n            this.render();\r\n        })\r\n    }\r\n\r\n    eventsMap(): {[key:string]: ()=>void}{\r\n        return{\r\n            'click:.set-age': this.onSetAgeClick\r\n        }\r\n    }\r\n\r\n    onSetAgeClick = (): void=>{\r\n        this.model.setRandomAge();\r\n        console.log('button was clicked');\r\n    }\r\n\r\n\r\n    template(): string {\r\n        return`\r\n        <div>\r\n        <h1>User Form</h1>\r\n        <div>User name: ${this.model.get('name')}</div>\r\n        <div>User age: ${this.model.get('age')}</div>\r\n\r\n        <input />\r\n        <button>Click Me</button>\r\n        <button class=\"set-age\">Set Random Age</button>\r\n        </div>\r\n        `\r\n    }\r\n\r\n    bindEvents(fragments:DocumentFragment): void {\r\n        const eventsMap = this.eventsMap();\r\n\r\n        for(let eventKey in eventsMap) {\r\n            const[eventName, selector] = eventKey.split(\":\");\r\n            fragments.querySelectorAll(selector).forEach(element =>{\r\n                element.addEventListener(eventName,eventsMap[eventKey]);\r\n            });\r\n        }\r\n    }\r\n\r\n    render(): void {\r\n        this.parent.innerHTML = '';\r\n        const templateElement = document.createElement('template');\r\n        templateElement.innerHTML = this.template();\r\n\r\n        this.bindEvents(templateElement.content);\r\n        this.parent.append(templateElement.content);\r\n\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"85e798e3ece4c90da00f257cc0bd72c5","cacheData":{"env":{}}}